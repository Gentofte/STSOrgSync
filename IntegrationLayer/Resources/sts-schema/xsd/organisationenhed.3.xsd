    <element name="Registrering" oio:definition="urn:oio:sagdok:organisationenhedregistrering:ADD:2.0.0" type="orgenhed:RegistreringType"/>
    <complexType name="RegistreringType" oio:definition="urn:oio:sagdok:organisationenhedregistrering:ADD:2.0.0">
        <complexContent>
            <extension base="sd:RegistreringType">
                <sequence>
                    <element oio:content="attributter" ref="orgenhed:AttributListe"/>
                    <element oio:content="tilstande" ref="orgenhed:TilstandListe"/>
                    <element oio:content="relationer" ref="orgenhed:RelationListe"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>

