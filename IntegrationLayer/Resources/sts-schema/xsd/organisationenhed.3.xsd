        <sequence>
            <element maxOccurs="unbounded" minOccurs="0" oio:content="gyldighed" ref="orgfaelles:Gyldighed"/>
            <element minOccurs="0" oio:content="lokaludvidelse" ref="sd:LokalUdvidelse"/>
        </sequence>
    </complexType>

    <element name="RelationListe" oio:definition="urn:oio:sagdok:organisationenhedrelationliste:ADD:2.0.0" type="orgenhed:RelationListeType"/>
    <complexType name="RelationListeType" oio:definition="urn:oio:sagdok:organisationenhedrelationliste:ADD:2.0.0">
