            <element minOccurs="0" oio:content="branche" ref="sd:Branche"/>
            <element minOccurs="0" oio:content="enhedstype" ref="sd:Enhedstype"/>
            <element maxOccurs="unbounded" minOccurs="0" oio:content="opgaver" ref="sd:Opgaver"/>
