        </sequence>
    </complexType>

    <element name="RelationListe" oio:definition="urn:oio:sagdok:organisationrelationliste:ADD:2.0.0" type="org:RelationListeType"/>
    <complexType name="RelationListeType" oio:definition="urn:oio:sagdok:organisationrelationliste:ADD:2.0.0">
        <sequence>
