    <element name="Organisation" oio:definition="urn:oio:sagdok:organisation:ADD:2.0.0" type="org:OrganisationType"/>
    <complexType name="OrganisationType" oio:definition="urn:oio:sagdok:organisation:ADD:2.0.0">
        <complexContent>
            <extension base="orgfaelles:AktoerType">
                <sequence>
                    <element maxOccurs="unbounded" oio:content="registreringer" ref="org:Registrering"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <element name="AttributListe" oio:definition="urn:oio:sagdok:organisationattributliste:ADD:2.0.0" type="org:AttributListeType"/>
    <complexType name="AttributListeType" oio:definition="urn:oio:sagdok:organisationattributliste:ADD:2.0.0">
        <sequence>
            <element maxOccurs="unbounded" minOccurs="0" oio:content="egenskaber" ref="org:Egenskab"/>
            <element minOccurs="0" oio:content="lokaludvidelse" ref="sd:LokalUdvidelse"/>
        </sequence>
    </complexType>

    <element name="TilstandListe" oio:definition="urn:oio:sagdok:organisationtilstandliste:ADD:2.0.0" type="org:TilstandListeType"/>
