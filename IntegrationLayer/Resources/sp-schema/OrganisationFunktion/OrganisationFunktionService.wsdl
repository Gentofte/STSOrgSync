    <wsdl:operation name="opret">
      <soap:operation soapAction="http://kombit.dk/sts/organisation/organisationfunktion/opret"/>
      <wsdl:input>
        <!-- input policy reference -->
        <wsp:PolicyReference URI="#Input_Policy"/>
        <soap:body use="literal"/>
      </wsdl:input>
      <wsdl:output>
